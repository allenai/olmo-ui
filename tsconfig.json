{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "es2019"],
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "sourceMap": true,
    "allowJs": true
  }
}
